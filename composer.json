{
    "name": "x-laravel/str-extend",
    "description": "Extends Laravel Str Facade with X-Adam/string package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehmet ÖĞMEN",
            "email": "www@mehmetogmen.com.tr",
            "homepage": "https://www.mehmetogmen.com.tr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": ">=5.5",
        "x-adam/string": "~2.0"
    },
    "autoload": {
        "classmap": [
            "./src/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "XLaravel\\StrExtendServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
